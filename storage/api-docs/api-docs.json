{
    "openapi": "3.0.0",
    "info": {
        "title": "Handassa",
        "version": "1.0.0"
    },
    "paths": {
        "/api/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Create Account",
                "operationId": "register",
                "requestBody": {
                    "description": "Register",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "first_name",
                                    "occupation_id",
                                    "last_name",
                                    "email",
                                    "phone_number",
                                    "password",
                                    "password_confirmation",
                                    "state",
                                    "city",
                                    "street",
                                    "latitude",
                                    "longitude"
                                ],
                                "properties": {
                                    "first_name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "John"
                                    },
                                    "last_name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Doe"
                                    },
                                    "occupation_id": {
                                        "type": "integer",
                                        "format": "integer",
                                        "example": "1"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "user@mail.com"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "format": "phone_number",
                                        "example": "+16472944676"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": ""
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "format": "string",
                                        "example": ""
                                    },
                                    "state": {
                                        "type": "string",
                                        "format": "string",
                                        "example": ""
                                    },
                                    "city": {
                                        "type": "string",
                                        "format": "string",
                                        "example": ""
                                    },
                                    "street": {
                                        "type": "string",
                                        "format": "string",
                                        "example": ""
                                    },
                                    "zipcode": {
                                        "type": "string",
                                        "format": "string",
                                        "example": ""
                                    },
                                    "latitude": {
                                        "type": "string",
                                        "format": "string",
                                        "example": ""
                                    },
                                    "longitude": {
                                        "type": "string",
                                        "format": "string",
                                        "example": ""
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "default": {
                        "description": "Success"
                    }
                }
            }
        }
    }
}