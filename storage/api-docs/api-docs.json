{
    "openapi": "3.0.0",
    "info": {
        "title": "Handassa",
        "version": "1.0.0"
    },
    "paths": {
        "/api/check_username": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Check Username",
                "operationId": "checkUsername",
                "requestBody": {
                    "description": "Check Username",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "username"
                                ],
                                "properties": {
                                    "username": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Person1"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "default": {
                        "description": "Success"
                    }
                }
            }
        },
        "/api/check_mobile_number": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Check Mobile Number",
                "operationId": "checkMobileNumber",
                "requestBody": {
                    "description": "Check Mobile Number",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "mobile_number"
                                ],
                                "properties": {
                                    "mobile_number": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "+921234567890"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "default": {
                        "description": "Success"
                    }
                }
            }
        },
        "/api/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Create Account",
                "operationId": "register",
                "requestBody": {
                    "description": "Register",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "role",
                                    "type",
                                    "username",
                                    "name",
                                    "mobile_number",
                                    "location"
                                ],
                                "properties": {
                                    "role": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "client"
                                    },
                                    "type": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "person"
                                    },
                                    "image": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "image.jpg"
                                    },
                                    "username": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "person1"
                                    },
                                    "name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "My Name"
                                    },
                                    "mobile_number": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "+921234567890"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "+921234567890 for freelancer company"
                                    },
                                    "location": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Al Doha"
                                    },
                                    "cr_copy": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "cr_copy.jpg for company"
                                    },
                                    "id_copy": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "id_copy.jpg for freelancer"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "default": {
                        "description": "Success"
                    }
                }
            }
        }
    }
}